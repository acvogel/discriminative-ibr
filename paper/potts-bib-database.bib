	Doi = {10.1145/2339530.2339653},
	Keywords = {a/b testing, controlled experiments, randomized experiments},
	Location = {Beijing, China},
	Pages = {786--794},
	Publisher = {ACM},
	Title = {Trustworthy Online Controlled Experiments: Five Puzzling Outcomes Explained},
	Year = {2012},
