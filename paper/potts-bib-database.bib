@article{Safir96,
	Abstract = {The distribution of weak crossover (WCO) effects is shown
                  to follow from a consistency condition on
                  $\overline{\text{A}}$-chains that prevents a single
                  $\overline{\text{A}}$-binder from simultaneously heading both
                  representational and derivational chains.  Chains with overt
                  pronominal tails are always representational, whether or not the
                  $\overline{\text{A}}$-chain head is quantificational, but chains
                  initially derived by movement are redefined as representational
                  chains if their chain heads are not true quantifiers, following
                  Lasnik and Stowell (1991).  The same consistency principle that
                  derives the WCO facts is then shown to derive from the distribution
                  of the secondary WCO effects discussed by Postal (1993) and
                  Safir (1984), as well as secondary strong crossover effects.},
	Author = {Safir, Ken},
	Journal = {Linguistic Inquiry},
