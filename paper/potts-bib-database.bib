@article{Tortora:Dikken:2009,
	Abstract = {In this paper we argue that variation in subject-verb agreement in Belfast English and Appalachian English bears on a number of issues that are relevant to current syntactic theory, including clause structure and subject positions, case, the syntax of negative polarity items and negative concord and the structure of the DP. We show that while the nature of the subject plays a role in determining subject-verb agreement in the two varieties (in different ways), subject position is the overt structural manifestation of the differing properties of the subject, independent of which properties are relevant in the variety in question. Our proposal on subject positions, which goes beyond what Henry (1995) proposed for Belfast English, is able to capture clusters of properties not discussed by Henry, and thus more fully accounts for the range of micro-parametric variation we find. This paper thus provides support for a configurational approach to subject-verb agreement.},
	Author = {Tortora, Christina and den Dikken, Marcel},
	Date-Added = {2014-01-12 20:06:43 +0000},
	Date-Modified = {2014-01-12 20:07:51 +0000},
	Journal = {Lingua},
	Keywords = {Correlations; Negative concord; Negative inversion; Negative polarity item (NPI); Singular concord; Subject-verb agreement},
	Number = {5},
	Pages = {1089--1108},
	Title = {Subject Agreement Variation: Support for the Configurational Approach},
	Volume = {120},
	Year = {2009},
	Bdsk-File-1 = {}}

